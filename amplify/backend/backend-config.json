{
  "api": {
    "efficienzaApi": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "defaultAuthentication": {
            "authenticationType": "API_KEY",
            "apiKeyConfig": {
              "apiKeyExpirationDays": 364,
              "description": "API for Efficienza.io"
            }
          },
          "additionalAuthenticationProviders": []
        }
      }
    }
  }
}